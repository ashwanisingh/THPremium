<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowBackground">@color/defaultWindowColor</item>
    </style>

    <style name="backBtnStyle">
        <item name="android:layout_width">@dimen/backBtnWidthHeight</item>
        <item name="android:layout_height">@dimen/backBtnWidthHeight</item>
        <item name="android:background">@null</item>
        <item name="android:src">@drawable/ic_back_copy_42</item>
        <item name="srcCompat">@drawable/ic_back_copy_42</item>
    </style>

</resources>
